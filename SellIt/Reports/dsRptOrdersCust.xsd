                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT     Orders.ID AS [invoice no], Orders.[Placement Date], Customers.FirstName + ' ' + Customers.LastName AS [Customer Name], Orders.[Order Date], 
                      Orders.Total, Orders.Discount, Payments.[Date] AS [payment date], Payments.Amount, Payments.Status, Products.Name AS product
FROM         ((((Orders INNER JOIN
                      [Order Details] ON Orders.ID = [Order Details].[Order]) INNER JOIN
                      Customers ON Orders.Customer = Customers.ID) INNER JOIN
                      Payments ON Orders.Payment = Payments.ID) INNER JOIN
                      Products ON [Order Details].Product = Products.ID)
WHERE     (Customers.ID = [@custid])</CommandText>
